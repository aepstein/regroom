<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(docket) -%>">
  <span class='title'>
    <%= docket.name %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), docket_url(docket),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_docket_path(docket),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_docket_path(docket),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => docket.name),
        :method => :delete %>
  </span>
</li>
