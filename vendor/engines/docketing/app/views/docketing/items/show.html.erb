<% content_for :body_content_title do %>
  <%= @docketing_item.name %>
<% end %>

<% content_for :body_content_left do %>
  <section>
    <h1>Name</h1>
    <p>
      <%=raw @docketing_item.name %>
    </p>
  </section>
<% end %>

<% content_for :body_content_right do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="docketing_items">
      <% @docketing_items.each do |item| %>
        <li>
          <%= link_to item.name, docketing_item_path(item) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render :partial => "/shared/content_page" %>

