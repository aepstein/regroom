<% content_for :body_content_title do %>
  <%= @docketing_docket.name %>
<% end %>

<% content_for :body_content_left do %>
  <section>
    <p>
      <%= t('.instruction') %>:
    </p>
    <%= render :partial => '/docketing/items/items',
               :object => @docketing_docket.items.roots %>
  </section>
<% end %>

<% content_for :body_content_right do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="docketing_dockets">
      <% @docketing_dockets.each do |docket| %>
        <li>
          <%= link_to docket.name, docketing_docket_url(docket) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render :partial => "/shared/content_page" %>

